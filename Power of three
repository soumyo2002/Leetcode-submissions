Link to the problem:https://leetcode.com/problems/power-of-three/

class Solution {
    public boolean isPowerOfThree(int n) {
        
        if(n==1)
            return true;
        
        if(n%3!=0 || n==0)
            return false;
        
        boolean ans=isPowerOfThree(n/3);
        return ans;
    }
}
